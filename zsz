			keep[2] = 'i';
				keep[3] = '\n';
					keep[4] = 'i';
	int fd;
	int i;
	fd = open("f.txt", O_RDWR, 0666);
	// while ((str = get_next_line(fd)) != NULL)
	// {
		get_keep(&ret, &keep);
		printf("%s", keep);
		// free(ret);
